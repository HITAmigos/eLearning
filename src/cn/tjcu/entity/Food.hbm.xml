<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cn.tjcu.entity.Food" table="Food">

	<id name="fid" column="fid">
		<generator class="increment"></generator>
	</id>
	
	<property name="fname" column="fname"></property>
	<property name="fimg" column="fimg"></property>
	<property name="fdescription" column="fdescription"></property>
	<property name="uid" column="uid"></property>
	<property name="fdate" column="fdate"></property>
	
	
	

</class>
</hibernate-mapping>