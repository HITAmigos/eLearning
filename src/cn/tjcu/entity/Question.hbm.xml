<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cn.tjcu.entity.Question" table="Question">

	<id name="qid" column="qid">
		<generator class="increment"></generator>
	</id>
	
	<property name="qname" column="qname"></property>
	<property name="qcontent" column="qcontent"></property>
	<property name="qanswer" column="qanswer"></property>
	<property name="qdate" column="qdate"></property>
	
	
	

</class>
</hibernate-mapping>