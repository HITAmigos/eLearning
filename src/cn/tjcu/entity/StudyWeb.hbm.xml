<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cn.tjcu.entity.StudyWeb" table="StudyWeb">

	<id name="wid" column="wid">
		<generator class="increment"></generator>
	</id>
	
	<property name="wname" column="wname"></property>
	<property name="wdescription" column="wdescription"></property>
	<property name="waddress" column="waddress"></property>

	
	
	

</class>
</hibernate-mapping>